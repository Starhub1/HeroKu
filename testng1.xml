<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Suite1" verbose="1">

    <listeners>
        <listener class-name="iniitialize.Listener"/>

    </listeners>


    <test name="Group" preserve-order="true" parallel="classes" thread-count="3">

        <classes>
            <class name="TestCase.DynamicControls"/>
            <class name="TestCase.DatePicker"/>
            <class name="TestCase.MouseHover"/>
            <class name="TestCase.DragNdrop"/>
            <class name="TestCase.ContextMenu"/>
            <class name="TestCase.DropDown"/>
            <class name="TestCase.CheckBoxes"/>
        </classes>
    </test>

</suite>